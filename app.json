{
  "expo": {
    "name": "react-native-final-project",
    "slug": "react-native-final-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "react-native-final-project",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "output": "server"
    },
    "extra": {
      "eas": {
        "projectId": "26c7ec02-cf1e-4cdd-b391-904f6a92cd5f"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "http://localhost:3000"
        }
      ]
    ]
  }
}
